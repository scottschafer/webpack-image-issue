{
  "name": "webpack-image-issue",
  "description": "Angularjs-typescript-webpack starter project",
  "license": "MIT",
  "scripts": {
    "prestart": "rimraf build_dist & copyfiles -f src/assets/* build_dist/",
    "start": "webpack-dev-server --mode=development --open --display-error-details",
    "start:dist": "webpack-dev-server --hot --inline --devtool=source-map --mode=production --optimize-minimize --open",
    "prebuild": "npm run prestart",
    "build": "webpack --mode=production --optimize-minimize --devtool=source-map",
    "test": "karma start --single-run",
    "test:dev": "karma start --browsers Chrome",
    "clean-build": "rimraf build_dist & copyfiles -f src/index.html build_dist/ & copyfiles -f src/assets/* build_dist/assets & npm run webpack-prod"
  },
  "devDependencies": {
    "@types/node": "^10.11.7",
    "babel-loader": "^8.0.4",
    "copyfiles": "2.1.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.4",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "^3.1.0",
    "jquery": "^3.3.1",
    "json-loader": "0.5.7",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-webpack": "^3.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "ng-annotate-loader": "0.6.1",
    "ng-annotate-patched": "1.7.0",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.9.2",
    "resolve-url-loader": "2.3.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "ts-loader": "^4.4.2",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "^3.1.3",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "webpack": "^4.17.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.6"
  },
  "dependencies": {}
